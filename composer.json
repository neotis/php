{
    "name": "neotis/neotis",
    "description": "Neotis Frameowrk",
    "type": "project",
    "keywords": ["framework", "neotis"],
    "license": "MIT",
    "require": {
        "ext-json": "*",
        "neotis/exception": "^1.0",
        "neotis/filesystem": "^1.0",
        "neotis/kernel": "^1.0.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4",
        "phpseclib/phpseclib": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
